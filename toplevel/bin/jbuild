(jbuild_version 1)
(executables
 ((names (jsoo_mkcmis jsoo_mktop jsoo_listunits))
  (libraries
   (
    bytes
    js_of_ocaml-compiler
    js_of_ocaml-compiler.findlib-support
    ))
  ))

(install
 ((section bin)
  (package js_of_ocaml-toplevel)
  (files ((jsoo_mktop.exe as jsoo_mktop)
          (jsoo_mkcmis.exe as jsoo_mkcmis)
          (jsoo_listunits.exe as jsoo_listunits)
          ))))
